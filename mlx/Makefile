#################
##  VARIABLES  ##
#################

#	Environment
OS :=						$(shell uname)

#	Output
NAME :=						libmlx.a

#	Compiler
CC :=						gcc
VERSION :=					-std=c11

ifeq ($(OS), Darwin)
	THREADS := 				$(shell sysctl -n hw.ncpu)
else
	THREADS :=				1
endif

FAST :=						-j$(THREADS)

HEADERS :=					-I ./include/
O_FLAG :=					-O2

#	Directories
OBJDIR :=					./build/

MLX_C_DIR :=				./mlx_c/
MLX_M_DIR :=				./mlx_m/

#	Sources
MLX_C +=					mlx_shaders.c mlx_xpm.c mlx_int_str_to_wordtab.c
MLX_M +=					mlx_new_window.m mlx_init_loop.m mlx_new_image.m

OBJECTS +=					$(patsubst %.c,$(OBJDIR)%.o,$(SRCS))
OBJECTS +=					$(patsubst %.m,$(OBJDIR)%.o,$(SRCS_M))

SRCS +=						$(MLX_C)
SRCS_M +=					$(MLX_M)

vpath %.c $(MLX_C_DIR)
vpath %.m $(MLX_M_DIR)

#################
##    RULES    ##
#################

all: $(NAME)

$(NAME): $(OBJECTS)
	@ar rcs $@ $(patsubst %.c,$(OBJDIR)%.o,$(notdir $(SRCS))) $(patsubst %.m,$(OBJDIR)%.o,$(notdir $(SRCS_M)))
	@ranlib $@
	@printf  "\033[92m\033[1:32mCompiling -------------> \033[91m$(NAME)\033[0m:\033[0m%-12s\033[32m[✔]\033[0m\n"

$(OBJECTS): | $(OBJDIR)

$(OBJDIR):
	@mkdir -p $@

$(OBJDIR)%.o: %.c
	@printf  "\033[1:92mCompiling $(NAME)\033[0m %-27s\033[32m[$<]\033[0m\n" ""
	@$(CC) $(VERSION) $(DEBUG)$(FLAGS)$(O_FLAG) $(HEADERS) -c $< -o $@
	@printf "\033[A\033[2K"
$(OBJDIR)%.o: %.m
	@printf  "\033[1:92mCompiling $(NAME)\033[0m %-27s\033[32m[$<]\033[0m\n" ""
	@$(CC) $(VERSION) $(DEBUG)$(FLAGS)$(O_FLAG) $(HEADERS) -c $< -o $@
	@printf "\033[A\033[2K"

clean:
	@/bin/rm -rf $(OBJDIR)
	@printf  "\033[1:32mCleaning object files -> \033[91m$(NAME)\033[0m\033[1:32m:\033[0m%-12s\033[32m[✔]\033[0m\n"

fast:
	@$(MAKE) $(FAST)

fclean: clean
	@/bin/rm -f $(NAME)
	@printf  "\033[1:32mCleaning binary -------> \033[91m$(NAME)\033[0m\033[1:32m:\033[0m%-12s\033[32m[✔]\033[0m\n"

re: fclean all

.PHONY: all cat clean fast fclean re

#################
##  WITH LOVE  ##
#################

